	<string>0.11</string>
	<key>Chrome</key>
	<dict/>
	<key>Content</key>
	<dict>
		<key>Scripts</key>
		<dict>
			<key>End</key>
			<array>
				<string>jquery-1.4.2.min.js</string>
				<string>flickr-zoom.js</string>
			</array>
		</dict>
